<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cn.exam.dao.mapper.zj.ZjSubjectInfoMapper">
    <select id="queryPageBySubject" resultType="cn.exam.domain.zj.ZjSubjectInfo">
        select subject_id subjectId , subject_name  subjectName
        from zj_subject_info
        order by subject_id
    </select>
</mapper>
